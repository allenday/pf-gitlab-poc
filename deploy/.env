# GitLab Local Development Environment Variables

# GitLab Configuration
GITLAB_HOST=gitlab.local
GITLAB_HTTP_PORT=80
GITLAB_HTTPS_PORT=443
GITLAB_SSH_PORT=2222

# GitLab Runner Configuration
GITLAB_RUNNER_REGISTRATION_TOKEN=
GITLAB_RUNNER_DESCRIPTION=local-runner

# Database Configuration (if using external DB)
POSTGRES_PASSWORD=gitlab_password
POSTGRES_USER=gitlab
POSTGRES_DB=gitlabhq_production

# Redis Configuration (if using external Redis)
REDIS_PASSWORD=

# Development Settings
GITLAB_ROOT_PASSWORD=password123
GITLAB_ROOT_EMAIL=admin@gitlab.local

# Docker Configuration
COMPOSE_PROJECT_NAME=gitlab-local